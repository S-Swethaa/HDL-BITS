module top_module (input x, input y, output z);
    wire [3:0]a;
    A A1(x,y,a[0]);
    B B1(x,y,a[1]);
    A A2(x,y,a[2]);
    B B2(x,y,a[3]);
    assign z=(a[0]|a[1])^(a[2]&a[3]);
endmodule
    
    module A(input x, input y, output z);
        assign z=(x^y)&x;
    endmodule
        module B(input x, input y, output z);
        assign z=~(x^y);
            
endmodule
